--langdef=coffee
--langmap=coffee:.coffee
--regex-coffee=/class ([A-Za-z.]+)/\1/c,class/
--regex-coffee=/^[ \t]([A-Z]+[a-z.]+)[ \t]/\1/o,object/
--regex-coffee=/([A-Za-z.]+)[ \t]*=.*->.*$/\1/f,function/
--regex-coffee=/([A-Za-z.]+)[ \t]*:.*->.*$/\1/m,method/
--regex-coffee=/(@[A-Za-z0-9.]+)[ \t]*=/\1/i,instancevar/
