[push]
  default = current

[alias]
  c = commit
  cm = commit -m
  co = checkout
  g = !git --no-pager grep --color -n
  l = log
  lo = log --graph --oneline --abbrev-commit
  ll = log --graph --pretty='format:%h | %an | %s'
  pr = pull --rebase
  pull-all = pull --recurse-submodules
  s = status
  d = diff
  serve = daemon --reuseaddr --verbose  --base-path=. --export-all ./.git
